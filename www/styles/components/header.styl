.l-site-nav
  position fixed
  width 100%
  height 50px
  z-index 100
  // background rgba($black, 0.075)
  // background $white
  
.logo-lockup
  position relative
  bottom 19px
  left 25px
  float left
  width 255px
   
  
.logo-lockup-fill
  fill $offWhite
  //fill $primary
    
.l-site-nav nav
  left 0
  width auto
  padding 0 2%
  ul
    min-height auto
    width auto
    float right
    border 0
    padding 0
    margin 0
    li
      display inline-block
      border-left 1px solid $offWhite
      padding-left 25px
      margin 10px 25px 0 0
      
      &:first-child
        border-left none
      a
        font-family 'Lato', sans-serif
        font-weight 400
        text-decoration none
        letter-spacing 2px
        color $offWhite
      
    
  

